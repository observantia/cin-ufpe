      <bpmn:outgoing>R2_Sim</bpmn:outgoing>
      <bpmn:outgoing>R2_Nao</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_SolicitarCorrecao" name="Solicitar correção ou falta">
      <bpmn:incoming>R2_Nao</bpmn:incoming>
      <bpmn:outgoing>R3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:sendTask id="Task_EnviarCoordenador" name="Enviar ao Coordenador">
      <bpmn:incoming>R3</bpmn:incoming>
    </bpmn:sendTask>
    <bpmn:task id="Task_AnalisarJuridica" name="Realizar análise jurídica">
      <bpmn:incoming>R2_Sim</bpmn:incoming>
      <bpmn:outgoing>R4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_JuridicamenteOk" name="Documentos juridicamente ok?">
      <bpmn:incoming>R4</bpmn:incoming>
      <bpmn:outgoing>R5_Sim</bpmn:outgoing>
      <bpmn:outgoing>R5_Nao</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_PermitirCorrecao" name="Permitir correção">
      <bpmn:incoming>R5_Nao</bpmn:incoming>
      <bpmn:outgoing>R6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:sendTask id="Task_EmitirParecer" name="Emitir parecer">
      <bpmn:incoming>R5_Sim</bpmn:incoming>
      <bpmn:incoming>R6</bpmn:incoming>
    </bpmn:sendTask>
    
    <bpmn:sequenceFlow id="R1" sourceRef="Task_VerificarConformidade" targetRef="Gateway_DocumentosOk" />
    <bpmn:sequenceFlow id="R2_Sim" name="SIM" sourceRef="Gateway_DocumentosOk" targetRef="Task_AnalisarJuridica" />
    <bpmn:sequenceFlow id="R2_Nao" name="NÃO" sourceRef="Gateway_DocumentosOk" targetRef="Task_SolicitarCorrecao" />
