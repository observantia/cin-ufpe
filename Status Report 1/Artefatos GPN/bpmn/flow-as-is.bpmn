        <bpmn:flowNodeRef>Gateway_JuridicamenteOk</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PermitirCorrecao</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EmitirParecer</bpmn:flowNodeRef>
