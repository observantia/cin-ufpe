  <bpmn:collaboration id="Collaboration_AsIs">
    <bpmn:participant id="Participant_Campus" name="IFPE Campus" processRef="Process_Campus" />
    <bpmn:participant id="Participant_Reitoria" name="IFPE Reitoria" processRef="Process_Reitoria" />
    <bpmn:messageFlow id="MsgFlow_1" sourceRef="Task_EnviarReitoria" targetRef="Task_VerificarConformidade" />
    <bpmn:messageFlow id="MsgFlow_2" sourceRef="Task_EnviarCoordenador" targetRef="Task_ConferirDocumentos" />
    <bpmn:messageFlow id="MsgFlow_3" sourceRef="Task_EmitirParecer" targetRef="Task_RevisarPedido" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Campus" name="Processo Campus" isExecutable="false">
    <bpmn:laneSet id="LaneSet_Campus">
      <bpmn:lane id="Lane_Demandante" name="Demandante/Interessado">
        <bpmn:flowNodeRef>Event_Start</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_IdentificarNecessidade</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Interessado" name="Interessado/Demandante">
        <bpmn:flowNodeRef>Task_LevantarNecessidades</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Analise</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DefinirProduto</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Coordenador" name="Coordenador de contratos">
        <bpmn:flowNodeRef>Task_ElaborarDocumentos</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EnviarReitoria</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ConferirDocumentos</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RevisarPedido</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PermitirEdital</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_End</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <bpmn:startEvent id="Event_Start" name="Demanda para contrato">
      <bpmn:outgoing>F1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_IdentificarNecessidade" name="Identificar necessidade">
      <bpmn:incoming>F1</bpmn:incoming>
      <bpmn:outgoing>F2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_LevantarNecessidades" name="Levantar as necessidades do requisitante">
      <bpmn:incoming>F2</bpmn:incoming>
      <bpmn:outgoing>F3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Analise" name="Análise das necessidades">
      <bpmn:incoming>F3</bpmn:incoming>
      <bpmn:outgoing>F4</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_DefinirProduto" name="Definir produto/serviço e valor consultado">
      <bpmn:incoming>F4</bpmn:incoming>
      <bpmn:outgoing>F5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_ElaborarDocumentos" name="Elaborar documentos">
      <bpmn:incoming>F5</bpmn:incoming>
      <bpmn:outgoing>F6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:sendTask id="Task_EnviarReitoria" name="Enviar para a Reitoria">
      <bpmn:incoming>F6</bpmn:incoming>
      <bpmn:outgoing>F7</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:task id="Task_ConferirDocumentos" name="Conferir documentos">
      <bpmn:incoming>F7</bpmn:incoming>
      <bpmn:outgoing>F8</bpmn:outgoing>
    </bpmn:task>
    <bpmn:receiveTask id="Task_RevisarPedido" name="Revisar pedido">
      <bpmn:incoming>F8</bpmn:incoming>
      <bpmn:outgoing>F9</bpmn:outgoing>
    </bpmn:receiveTask>
    <bpmn:sendTask id="Task_PermitirEdital" name="Permitir Edital">
      <bpmn:incoming>F9</bpmn:incoming>
      <bpmn:outgoing>F10</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:endEvent id="Event_End" name="Publicar Edital">
      <bpmn:incoming>F10</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="F1" sourceRef="Event_Start" targetRef="Task_IdentificarNecessidade" />
    <bpmn:sequenceFlow id="F2" sourceRef="Task_IdentificarNecessidade" targetRef="Task_LevantarNecessidades" />
    <bpmn:sequenceFlow id="F3" sourceRef="Task_LevantarNecessidades" targetRef="Gateway_Analise" />
    <bpmn:sequenceFlow id="F4" sourceRef="Gateway_Analise" targetRef="Task_DefinirProduto" />
    <bpmn:sequenceFlow id="F5" sourceRef="Task_DefinirProduto" targetRef="Task_ElaborarDocumentos" />
    <bpmn:sequenceFlow id="F6" sourceRef="Task_ElaborarDocumentos" targetRef="Task_EnviarReitoria" />
    <bpmn:sequenceFlow id="F7" sourceRef="Task_EnviarReitoria" targetRef="Task_ConferirDocumentos" />
    <bpmn:sequenceFlow id="F8" sourceRef="Task_ConferirDocumentos" targetRef="Task_RevisarPedido" />
    <bpmn:sequenceFlow id="F9" sourceRef="Task_RevisarPedido" targetRef="Task_PermitirEdital" />
    <bpmn:sequenceFlow id="F10" sourceRef="Task_PermitirEdital" targetRef="Event_End" />
  </bpmn:process>
  
