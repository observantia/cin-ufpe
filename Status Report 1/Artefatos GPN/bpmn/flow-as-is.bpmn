    <bpmn:receiveTask id="Task_VerificarConformidade" name="Verificar conformidade dos documentos">
      <bpmn:outgoing>R1</bpmn:outgoing>
    </bpmn:receiveTask>
    <bpmn:exclusiveGateway id="Gateway_DocumentosOk" name="Documentos ok?">
